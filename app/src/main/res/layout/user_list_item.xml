<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="8dp"
    app:cardElevation="4dp"
    android:layout_marginTop="20dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:layout_marginBottom="-80dp">

        <ImageView
            android:id="@+id/imageView26"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:scaleType="centerCrop"
            android:src="@drawable/shape_11" />

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageView26"
            android:layout_alignParentStart="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="-151dp"
            android:src="@drawable/icon_5" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_toEndOf="@+id/imageView33"
            android:text="@string/S1"
            android:textColor="@color/white"
            android:textSize="@dimen/Name" />


        <Button
            android:id="@+id/button7"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView39"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="194dp"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="90dp"
            android:layout_toEndOf="@+id/imageView33"
            android:backgroundTint="@color/btn_orange"
            android:text="@string/Ad_Users_delete_btn"
            android:textSize="@dimen/Btn_name" />

    </RelativeLayout>

</androidx.cardview.widget.CardView>
